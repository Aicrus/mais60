{
  "expo": {
    "owner": "aicrustech",
    "name": "Mais 60",
    "slug": "mais60",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/logo-mais60.png",
    "scheme": "mais60",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.seudominio.mais60",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Precisamos acessar sua biblioteca para escolher uma foto de perfil.",
        "NSLocationWhenInUseUsageDescription": "Usamos sua localização opcionalmente para registrar caminhadas e melhorar suas estatísticas.",
        "NSMotionUsageDescription": "Usamos os sensores de movimento para estimar passos e atividade.",
        "UISupportedInterfaceOrientations": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight",
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ],
        "UISupportedInterfaceOrientations~ipad": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight",
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationLandscapeLeft",
          "UIInterfaceOrientationLandscapeRight"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-mais60.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.seudominio.mais60",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACTIVITY_RECOGNITION",
        "POST_NOTIFICATIONS",
        "READ_MEDIA_IMAGES",
        "READ_EXTERNAL_STORAGE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACTIVITY_RECOGNITION",
        "POST_NOTIFICATIONS",
        "READ_MEDIA_IMAGES",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "Mais 60 — Saúde e bem-estar para pessoas 60+",
      "description": "Aplicativo de saúde e bem-estar focado em pessoas com mais de 60 anos: acompanhamento de hábitos, consultas e qualidade de vida.",
      "lang": "pt-BR",
      "themeColor": "#2563eb",
      "backgroundColor": "#ffffff",
      "templatePath": "./web/index.html",
      "build": {
        "minify": true,
        "babel": {
          "useTransformRuntime": true
        }
      },
      "performance": true,
      "splashScreenOptions": {
        "backgroundColor": "#f9fafb",
        "darkMode": {
          "backgroundColor": "#111827"
        }
      },
      "optimization": {
        "treeshaking": true,
        "splitChunks": true
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logo Mais 60 Roxo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/images/Logo Mais 60 Branco.png",
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      "expo-localization",
      "expo-web-browser",
      "expo-maps"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "5e50025f-fa27-4cbf-b237-c531b3c14c1e"
      }
    },
    "fonts": [],
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/5e50025f-fa27-4cbf-b237-c531b3c14c1e"
    }
  }
}
